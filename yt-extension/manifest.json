
{
  "manifest_version": 3,
  "name": "YouTube Video Analyzer",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.jpeg",
      "48": "icon.jpeg",
      "128": "icon.jpeg"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ]
}
